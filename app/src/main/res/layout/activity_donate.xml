<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary">

    <TextView
        android:id="@+id/tSettings"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="0dp"
        android:text="@string/donateAct"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="65sp"
        android:textStyle="bold"
        tools:ignore="TextSizeCheck" />

    <ImageButton
        android:id="@+id/menuButton"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:background="@null"
        android:contentDescription="@string/todo1"
        android:src="@drawable/ic_menu"
        tools:ignore="ImageContrastCheck" />

    <View
        android:id="@+id/vgreenRec"
        android:layout_width="match_parent"
        android:layout_height="540dp"
        android:layout_above="@id/vgreenRecDivider"
        android:background="@drawable/rounded_rectangle" />

    <View
        android:id="@+id/vgreenRecDivider"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_alignParentBottom="true"/>

    <TextView
        android:id="@+id/tvName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="32dp"
        android:layout_toEndOf="@+id/circle_view"
        android:text="@string/rufelle"
        android:textColor="@color/white"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/tvAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="13dp"
        android:layout_toStartOf="@+id/menuButton"
        android:gravity="end"
        android:text="@string/lahug_cebu_city"
        android:textColor="@color/white"
        android:textSize="15sp"
        tools:ignore="RelativeOverlap" />

    <View
        android:id="@+id/circle_view"
        android:layout_width="64dp"
        android:layout_height="65dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="11dp"
        android:background="@drawable/account_circle" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignTop="@+id/vgreenRec"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="237dp"
        android:layout_marginBottom="361dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="@string/DONATE"
        app:srcCompat="@drawable/catsofitpark"
        tools:ignore="DuplicateClickableBoundsCheck" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_above="@+id/vgreenRecDivider"
        android:layout_alignTop="@+id/vgreenRec"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="237dp"
        android:layout_marginBottom="361dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="@string/DONATE"
        app:srcCompat="@drawable/catsofitpark" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_above="@+id/vgreenRecDivider"
        android:layout_alignTop="@+id/vgreenRec"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="288dp"
        android:layout_marginEnd="236dp"
        android:layout_marginBottom="133dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="@string/DONATE"
        app:srcCompat="@drawable/savingstrayscebu" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_above="@+id/vgreenRecDivider"
        android:layout_alignTop="@+id/vgreenRec"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="232dp"
        android:layout_marginTop="288dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="133dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="@string/DONATE"
        app:srcCompat="@drawable/catsdongilargao" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_above="@+id/vgreenRecDivider"
        android:layout_alignTop="@+id/vgreenRec"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="232dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="361dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="@string/DONATE"
        app:srcCompat="@drawable/happytails" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="186dp"
        android:layout_height="29dp"
        android:layout_above="@+id/vgreenRecDivider"
        android:layout_alignTop="@+id/vgreenRec"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="196dp"
        android:layout_marginEnd="208dp"
        android:layout_marginBottom="315dp"
        android:fontFamily="sans-serif-black"
        android:text="Cats of IT Park"
        android:textAlignment="center"
        android:textColor="#854F6C"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="184dp"
        android:layout_height="50dp"
        android:layout_above="@+id/vgreenRecDivider"
        android:layout_alignTop="@+id/vgreenRec"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="205dp"
        android:layout_marginTop="427dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="63dp"
        android:fontFamily="sans-serif-black"
        android:text="Cats of Don Gil Argao"
        android:textAlignment="center"
        android:textColor="#854F6C"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="49dp"
        android:layout_above="@+id/vgreenRecDivider"
        android:layout_alignTop="@+id/vgreenRec"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="427dp"
        android:layout_marginEnd="208dp"
        android:layout_marginBottom="64dp"
        android:fontFamily="sans-serif-black"
        android:text="Saving Strays Cebu"
        android:textAlignment="center"
        android:textColor="#854F6C"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="193dp"
        android:layout_height="29dp"
        android:layout_above="@+id/vgreenRecDivider"
        android:layout_alignTop="@+id/vgreenRec"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="204dp"
        android:layout_marginTop="196dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="315dp"
        android:fontFamily="sans-serif-black"
        android:text="Happy Tails"
        android:textAlignment="center"
        android:textColor="#854F6C"
        android:textSize="20sp" />

</RelativeLayout>
